function D3m0n_4U () {

echo "-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-"
echo "*              Enter Next Command                   *"
echo "-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-"
echo
echo
####### TARGET DEFINED
target=$1

echo "Now Choose an option"

####### OPTIONS TIME

echo "1.  Ping Target" 
# iske further 2 part honge

echo "2.  Reverse IP Lookup"
echo "3.  Traceroute Target"
echo "4.  NSlookup Target"
echo "5.  Check for Whois"
echo "6.  Find Directories"
#ask directory option

echo "7.  Find SubDomains"
echo "8.  Find emails with domain"
# Ask for Dictionary

echo "9.  Dig Target"

echo "10. Advanced Recon"
#iske further parts
echo "99. Bye Bye!!!"
echo "0.  Change Target"
#echo "11. Nmap Scan"
#further parts
echo
echo "Enter Your Option :-" 
echo
read option
echo
####### OPTION DEFINED

if [ $option == 1 ] 
then 
#ye png lie h
	echo
	echo "Choose One Option"
	echo
	echo "1. normal ping"
	echo "2. ping with packet size"
	echo
	read ping_opt
	
			if [ $ping_opt == 1 ]
		 then
			xterm -e "ping -c3 $target; read abc"& d3_re $target $option
		
		elif [ $ping_opt == 2 ]
		then

		echo
			echo "tell packet size"
	echo		
	read pack_size
		xterm -e "ping -c 3 -M do -s $(( $pack_size - 28 )) $target; read abc"& d3_re $target $option
		else
			xterm -e "echo 'Choose 1 or 2';read abc"& d3_re $target $option 
fi

#######yaha ping lie pura ho gya normal and MTU size dekhne lie

elif [ $option == 2 ]
then
xterm -e "curl https://api.hackertarget.com/reverseiplookup/?q=$target; read abc"& D3m0n_4U $target
echo
#######yaha point 2 reverse ip domain check

elif [ $option == 3 ]
then
xterm -e "traceroute $target; read abc"& D3m0n_4U $target

####### yaha trace route

elif [ $option == 4 ]
then 
xterm -e "nslookup $target; read abc"& D3m0n_4U $target

####### yaha nslookup complete

elif [ $option == 5 ]
then
xterm -e "whois $target; read abc"& D3m0n_4U $target

####### whois complete

elif [ $option == 6 ]
then
echo
echo "Do you want to give custom wordlist?"
echo "0. NO"
echo "1. YES"
echo
read ans

####### yaha wordlist puchi

	echo
	if [ $ans == 1 ]
	then
	echo "Please provide Location"
	read location
	xterm -e "dirb https://$target $location; read abc"& d3_re $target $option

	elif [ $ans == 0 ]
	then
	xterm -e "dirb https://$target; read abc"& d3_re $target $option

	else
	xterm -e "echo 'choose 0/1';read abc"& d3_re $target $option

fi

####### dirb completed

elif [ $option == 7 ]
then
xterm -e "sublist3r -d $target; read abc"& D3m0n_4U $target

####### Subdomain complete

elif [ $option == 8 ]
then
echo
echo "Choose One option"
echo
echo "1. Anubis"
echo "2. Baidu"
echo "3. Bing" 
echo "4. Binaryedge" 
echo "5. Bingapi" 
