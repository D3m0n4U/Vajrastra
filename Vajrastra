	echo
	read ping_opt

			if [ $ping_opt == 1 ]
		 then
			xterm -e "ping -c3 $target; read abc"& d3_re $target $option

		elif [ $ping_opt == 2 ]
		then

		echo
			echo "tell packet size"
	echo
	read pack_size
