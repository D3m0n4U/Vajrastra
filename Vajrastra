        then
        xterm -e "theHarvester -d $target -b trello; read abc"& d3_re $target $option 


        elif [ $harv_opt == 33 ]
        then
        xterm -e "theHarvester -d $target -b twitter; read abc"& d3_re $target $option 


        elif [ $harv_opt == 34 ]
        then
        xterm -e "theHarvester -d $target -b urlscan; read abc"& d3_re $target $option  


        elif [ $harv_opt == 35 ]
        then
        xterm -e "theHarvester -d $target -b virustotal; read abc"& d3_re $target $option  


        elif [ $harv_opt == 36 ]
        then
        xterm -e "theHarvester -d $target -b yahoo; read abc"& d3_re $target $option 


